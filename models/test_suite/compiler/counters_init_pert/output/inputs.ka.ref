%def: "seed" "92067277"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "t.u."
%def: "outputFileName" "data.csv"


%agent: A(x{u p} c{=1/+=10})

%plot: [T]
A(x{u/p}[.] c{>=0/+=1}),  @ 1
/*0*/%mod: (|A(c{=11})| = 1) do $PRINTF "
Counter c of agent A reached maximum
"; $STOP "counter_perturbation.ka"; repeat [false]
/*1*/%mod: (|A(c{=2})| > 1) do $APPLY 10 A(x{u}[.] c{=1})+; repeat [false]

%init: 10 A(x{u}[.] c{=1})

%mod: [E] = 10 do $STOP;
