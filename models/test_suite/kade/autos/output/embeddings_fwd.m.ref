function main=main()
% command line: 
%      'KaDE' 'autos.ka' '-d' 'output' '--output' 'embeddings_fwd''--with-symmetries' 'Forward' '-l' '1' '-p' '0.01' '--rate-convention''KaSim' '--output-plot''embeddings_fwd.csv'
%% THINGS THAT ARE KNOWN FROM KAPPA FILE AND KaSim OPTIONS:
%% 
%% init - the initial abundances of each species and token
%% tinit - the initial simulation time (likely 0)
%% tend - the final simulation time 
%% initialstep - initial time step at the beginning of numerical integration
%% maxstep - maximal time step for numerical integration
%% reltol - relative error tolerance;
%% abstol - absolute error tolerance;
%% period - the time period between points to return
%%
%% variables (init(i),y(i)) denote numbers of embeddings 
%% no correcion is applied on rule rates


tinit=0;
tend=1;
initialstep=1e-05;
maxstep=0.02;
reltol=0.001;
abstol=0.001;
period=0.01;
nonnegative=false;

global nodevar
nodevar=5;
global max_stoc_coef
max_stoc_coef=0;
nvar=6;
nobs=4;
nrules=3;

global var
var=zeros(nvar,1);
global init
init=sparse(nodevar,1);
stoc=zeros(nrules,max_stoc_coef);
global k
global kd
global kun
global kdun
global stoc

k=zeros(nrules,1);
kd=zeros(nrules,1);
kun=zeros(nrules,1);
kdun=zeros(nrules,1);
global jacvar
jacvar=sparse(nvar,nodevar);
global jack
global jackd
global jackun
global jackund
global jacstoc

jack=zeros(nrules,nodevar);
jackd=zeros(nrules,nodevar);
jackun=zeros(nrules,nodevar);
jackund=zeros(nrules,nodevar);

t = 0.000000;

init(5)=t;
var(5)=init(2); % x_dimer
init(1)=10; % A(x, y)
var(4)=init(1)+init(4)+init(3); % x_free
var(3)=init(1)+init(4)+init(2)+2*init(3); % cst
var(2)=init(1)+init(4)+init(2)+2*init(3); % cstt
var(1)=1; % k


uiIsOctave = false;
uiIsMatlab = false;
LIC = license('inuse');
for elem = 1:numel(LIC)
    envStr = LIC(elem).feature
    if strcmpi(envStr,'octave')
       LICname=envStr;
       uiIsOctave = true;
       break
    end
    if strcmpi(envStr,'matlab')
       LICname=envStr
       uiIsMatlab = true;
       break
    end
end


if nonnegative 
   options = odeset('RelTol', reltol, ...
                    'AbsTol', abstol, ...
                    'InitialStep', initialstep, ...
                    'MaxStep', maxstep, ...
                    'Jacobian', @ode_jacobian, ...
                   'NonNegative', [1:1:4]);
else
   options = odeset('RelTol', reltol, ...
                    'AbsTol', abstol, ...
                    'InitialStep', initialstep, ...
                    'MaxStep', maxstep, ...
                    'Jacobian', @ode_jacobian);
end


if nonnegative
   if uiIsMatlab
      soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
      soln.y=soln.y';
      vt = soln.x;
      vy = soln.y;
   elseif uiIsOctave
      [vt,vy] = ode23s(@ode_aux,[tinit tend],ode_init(),options);
   end
else
   if uiIsMatlab
      soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
      soln.y=soln.y';
      vt = soln.x;
      vy = soln.y;
   elseif uiIsOctave
      soln = ode2r(@ode_aux,[tinit tend],ode_init(),options);
      vt = soln.x;
      vy = soln.y;
   end
end;


nrows = length(vt);

tmp = zeros(nodevar,1);

n_points = floor ((tend-tinit)/period)+1;
t = linspace(tinit, tend, n_points);
obs = zeros(nrows,nobs);

for j=1:nrows
    for i=1:nodevar
        z(i)=vy(j,i);
    end
    h=ode_obs(z);
    for i=1:nobs
        obs(j,i)=h(i);
    end
end
if nobs==1
   y = interp1(vt, obs, t, 'pchip')';
else
   y = interp1(vt, obs, t, 'pchip');
end


filename = 'embeddings_fwd.csv';
fid = fopen (filename,'w');
fprintf(fid,'# KaDE autos.ka -d output --output embeddings_fwd --with-symmetries Forward -l 1 -p 0.01 --rate-convention KaSim --output-plot embeddings_fwd.csv\n')
fprintf(fid,'# ')
fprintf(fid,'[T],')
fprintf(fid,'cst,')
fprintf(fid,'x_free,')
fprintf(fid,'x_dimer,')
fprintf(fid,'\n')
for j=1:n_points
    for i=1:nobs
        fprintf(fid,'%f,',y(j,i));
    end
    fprintf(fid,'\n');
end
fclose(fid);


end



function Init=ode_init()

global nodevar
global init
Init=zeros(nodevar,1);

Init(1) = init(1); % A(x, y)
Init(2) = init(2); % A(x!1, y), A(x!1, y)
Init(3) = init(3); % A(x!1, y), A(x, y!1)
Init(4) = init(4); % A(x, y!1), A(x, y!1)
Init(5) = init(5); % t
end


function dydt=ode_aux(t,y)

global nodevar
global max_stoc_coef
global var
global k
global kd
global kun
global kdun
global stoc

var(5)=y(2); % x_dimer
var(4)=y(1)+y(4)+y(3); % x_free
var(3)=y(1)+y(4)+y(2)+2*y(3); % cst
var(2)=y(1)+y(4)+y(2)+2*y(3); % cstt

k(1)=var(2);
k(2)=var(1);
k(3)=var(1);

dydt=zeros(nodevar,1);

% rule    : A(x,y), A(x,y) -> A(x,y!1), A(x,y!1)
% reaction: A(x, y) + A(x, y) -> A(x, y!1), A(x, y!1) 

dydt(1)=dydt(1)-k(3)*y(1)*y(1);
dydt(1)=dydt(1)-k(3)*y(1)*y(1);
dydt(4)=dydt(4)+2*k(3)*y(1)*y(1);

% rule    : A(x,y), A(x,y) -> A(x!1,y), A(x,y!1)
% reaction: A(x, y) + A(x, y) -> A(x!1, y), A(x, y!1) 

dydt(1)=dydt(1)-k(2)*y(1)*y(1);
dydt(1)=dydt(1)-k(2)*y(1)*y(1);
dydt(3)=dydt(3)+k(2)*y(1)*y(1);

% rule    : A(x,y), A(x,y) -> A(x!1,y), A(x!1,y)
% reaction: A(x, y) + A(x, y) -> A(x!1, y), A(x!1, y) 

dydt(1)=dydt(1)-k(1)*y(1)*y(1);
dydt(1)=dydt(1)-k(1)*y(1)*y(1);
dydt(2)=dydt(2)+2*k(1)*y(1)*y(1);
dydt(5)=1;

end


function jac=ode_jacobian(t,y)

global nodevar
global max_stoc_coef
global jacvar
global var
global k
global kd
global kun
global kdun
global stoc

global jack
global jackd
global jackun
global jackund
global jacstoc

var(5)=y(2); % x_dimer
var(4)=y(1)+y(4)+y(3); % x_free
var(3)=y(1)+y(4)+y(2)+2*y(3); % cst
var(2)=y(1)+y(4)+y(2)+2*y(3); % cstt

k(1)=var(2);
k(2)=var(1);
k(3)=var(1);
jacvar(5,2)=2;
jacvar(4,1)=1;
jacvar(4,3)=1;
jacvar(4,4)=2;
jacvar(3,1)=1;
jacvar(3,2)=2;
jacvar(3,3)=2;
jacvar(3,4)=2;
jacvar(2,1)=1;
jacvar(2,2)=2;
jacvar(2,3)=2;
jacvar(2,4)=2;

jack(1,1)=jacvar(2,1);
jack(1,2)=jacvar(2,2);
jack(1,3)=jacvar(2,3);
jack(1,4)=jacvar(2,4);

jac=sparse(nodevar,nodevar);

% rule    : A(x,y), A(x,y) -> A(x,y!1), A(x,y!1)
% reaction: A(x, y) + A(x, y) -> A(x, y!1), A(x, y!1) 

jac(1,1)=jac(1,1)-k(3)*y(1);
jac(1,1)=jac(1,1)-k(3)*y(1);
jac(1,1)=jac(1,1)-k(3)*y(1);
jac(1,1)=jac(1,1)-k(3)*y(1);
jac(4,1)=jac(4,1)+2*k(3)*y(1);
jac(4,1)=jac(4,1)+2*k(3)*y(1);

% rule    : A(x,y), A(x,y) -> A(x!1,y), A(x,y!1)
% reaction: A(x, y) + A(x, y) -> A(x!1, y), A(x, y!1) 

jac(1,1)=jac(1,1)-k(2)*y(1);
jac(1,1)=jac(1,1)-k(2)*y(1);
jac(1,1)=jac(1,1)-k(2)*y(1);
jac(1,1)=jac(1,1)-k(2)*y(1);
jac(3,1)=jac(3,1)+k(2)*y(1);
jac(3,1)=jac(3,1)+k(2)*y(1);

% rule    : A(x,y), A(x,y) -> A(x!1,y), A(x!1,y)
% reaction: A(x, y) + A(x, y) -> A(x!1, y), A(x!1, y) 

jac(1,1)=jac(1,1)-jack(1,1)*y(1)*y(1);
jac(1,2)=jac(1,2)-jack(1,2)*y(1)*y(1);
jac(1,3)=jac(1,3)-jack(1,3)*y(1)*y(1);
jac(1,4)=jac(1,4)-jack(1,4)*y(1)*y(1);
jac(1,1)=jac(1,1)-k(1)*y(1);
jac(1,1)=jac(1,1)-k(1)*y(1);
jac(1,1)=jac(1,1)-jack(1,1)*y(1)*y(1);
jac(1,2)=jac(1,2)-jack(1,2)*y(1)*y(1);
jac(1,3)=jac(1,3)-jack(1,3)*y(1)*y(1);
jac(1,4)=jac(1,4)-jack(1,4)*y(1)*y(1);
jac(1,1)=jac(1,1)-k(1)*y(1);
jac(1,1)=jac(1,1)-k(1)*y(1);
jac(2,1)=jac(2,1)+2*jack(1,1)*y(1)*y(1);
jac(2,2)=jac(2,2)+2*jack(1,2)*y(1)*y(1);
jac(2,3)=jac(2,3)+2*jack(1,3)*y(1)*y(1);
jac(2,4)=jac(2,4)+2*jack(1,4)*y(1)*y(1);
jac(2,1)=jac(2,1)+2*k(1)*y(1);
jac(2,1)=jac(2,1)+2*k(1)*y(1);
end


function obs=ode_obs(y)

global nobs
global var
obs=zeros(nobs,1);

t = y(5);
var(5)=y(2); % x_dimer
var(4)=y(1)+y(4)+y(3); % x_free
var(3)=y(1)+y(4)+y(2)+2*y(3); % cst
var(2)=y(1)+y(4)+y(2)+2*y(3); % cstt

obs(1)=t; % [T]
obs(2)=var(3); % cst
obs(3)=var(4); % x_free
obs(4)=var(5); % x_dimer

end


main();

