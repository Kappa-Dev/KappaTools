%agent: A(s1{u,p},s2{u,p},c{=0 / +=2})
%init:	40 A()

/*%var: 'k' |A(x{<=1})|*/
A(s1{u},c{=x}) -> A(s1{p},c{+=1}) @ 3*7^x
A(s2{u}[.],c{=x}) -> A(s2{p}[.],c{+=1}) @ 3*7^x

/*A(s1[.]{p},c{=x}) -> A(s1[.]{u},c{+= -1}) @ 2*5^x
A(s2{p},c{=x}) -> A(s2{u},c{+= -1}) @ 2*5^x*/

A(c{>=2}) -> A(c{+= 1}) @ |A(c{>=1})|
A(c{>=3}) -> A(c{+= 1}) @ |A(c{<=1})|

%obs: 'k' |A(c{<=1})|
%obs: 'kk' |A()|

%init: 3 A()
