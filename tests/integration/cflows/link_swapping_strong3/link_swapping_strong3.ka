%def: "displayCompression" "weak" "strong"

A(x[.]),A(x[.]) -> A(x[1]),A(x[1]) @ 1
A(y[.],x[1]),A(y[.],x[1]) -> A(y[1],x[2]),A(y[1],x[2]) @ 1
A(x[1],y[2]),A(x[1],y[2]) -> A(x[1],y[2]),A(x[2],y[1]) @ 1

'GOAL' A(x[1]),A(y[1]) -> A(x[.]),A(y[.]) @ 1

%init: 8 A(x[.],y[.])

%agent: A(x,y)

%mod: [true] do $TRACK 'GOAL' [true];

