dydt(3)=dydt(3)+k(6)*y(7)/2;
dydt(3)=dydt(3)+k(6)*y(7)/2;

% rule    : R(r[_]) -> .
% reaction: R(e[1],r[2]).R(e[3],r[2]).E(r[1],s[.]).E(r[3],s[.]) -> E(r[.],s[.]) + R(e[1],r[.]).E(r[1],s[.])

